<?xml version="1.0" encoding="utf-8"?>
<xs-formulas xmlns:ixml="http://invisiblexml.org/NS" ixml:state="ambiguous">
   <comment> Some basic formulas </comment>
   <comment> Propositional variables </comment>
   <formula>
      <prop-var name="p"/>
   </formula>
   <formula>
      <prop-var name="q"/>
   </formula>
   <formula>
      <prop-var name="Prop-2a"/>
   </formula>
   <formula>
      <prop-var name="φ"/>
   </formula>
   <formula>
      <prop-var name="א"/>
   </formula>
   <comment> and </comment>
   <comment> Propositional constants </comment>
   <formula>
      <prop-constant name="Prop-2a"/>
   </formula>
   <formula>
      <prop-constant name="Frege-was-born-in-1848"/>
   </formula>
   <formula>
      <prop-constant name="Russell-died-in-1970"/>
   </formula>
   <formula>
      <prop-constant name="Prop-2b"/>
   </formula>
   <comment> quoted strings as propositional constants </comment>
   <formula>
      <prop-constant string="Frege was born in 1848"/>
   </formula>
   <formula>
      <prop-constant string="he is at his desk"/>
   </formula>
   <formula>
      <prop-constant string="he is eating lunch"/>
   </formula>
   <comment> Parenthesized basic formulas </comment>
   <formula>
      <prop-var name="q"/>
   </formula>
   <formula>
      <prop-var name="Prop-2a"/>
   </formula>
   <formula>
      <prop-var name="φ"/>
   </formula>
   <formula>
      <prop-constant name="Prop-2a"/>
   </formula>
   <comment> Compound formulas </comment>
   <formula>
      <implies IMPLIES="onlyif">
         <iff IFF="iff">
            <prop-var name="p"/>
            <and AND="&amp;">
               <prop-var name="q"/>
               <prop-var name="r"/>
            </and>
         </iff>
         <and AND="&amp;&amp;">
            <prop-var name="p"/>
            <prop-var name="q"/>
         </and>
      </implies>
   </formula>
   <formula>
      <iff IFF="iff">
         <implies IMPLIES="implies">
            <prop-var name="p"/>
            <and AND="&amp;">
               <prop-var name="q"/>
               <prop-var name="r"/>
            </and>
         </implies>
         <and AND="∧">
            <prop-var name="p"/>
            <prop-var name="q"/>
         </and>
      </iff>
   </formula>
   <formula>
      <implies IMPLIES="implies">
         <or OR="∨">
            <prop-var name="p"/>
            <prop-var name="q"/>
         </or>
         <prop-var name="r"/>
      </implies>
   </formula>
   <formula>
      <or OR="∨">
         <prop-var name="p"/>
         <implies IMPLIES="implies">
            <prop-var name="q"/>
            <prop-var name="r"/>
         </implies>
      </or>
   </formula>
   <formula>
      <implies IMPLIES="implies">
         <prop-var name="p"/>
         <or OR="or">
            <prop-var name="q"/>
            <prop-var name="r"/>
         </or>
      </implies>
   </formula>
   <formula>
      <and AND="and">
         <prop-constant string="Jones is here"/>
         <prop-constant string="Smith is away"/>
      </and>
   </formula>
   <formula>
      <implies IMPLIES="⇒">
         <iff IFF="⇔">
            <or OR="∨">
               <and AND="∧">
                  <or OR="∨">
                     <prop-var name="p"/>
                     <prop-var name="q"/>
                  </or>
                  <or OR="∨">
                     <prop-var name="p"/>
                     <not NOT="-">
                        <prop-var name="q"/>
                     </not>
                  </or>
               </and>
               <and AND="∧">
                  <not NOT="-">
                     <prop-var name="p"/>
                  </not>
                  <prop-var name="q"/>
               </and>
            </or>
            <prop-var name="q"/>
         </iff>
         <or OR="∨">
            <and AND="∧">
               <prop-var name="p"/>
               <prop-var name="r"/>
            </and>
            <and AND="∧">
               <prop-var name="p"/>
               <not NOT="~">
                  <prop-var name="r"/>
               </not>
            </and>
         </or>
      </implies>
   </formula>
</xs-formulas>